{% extends "base.twig" %}

{% block title %}{{sheet.title}}{% endblock %}
{% block content %}
    <div class="row">
        <div class="box box-solid">
            <div class="box-header">
                <h3 class="box-title">{{sheet.title}}</h3>
            </div><!-- /.box-header -->
            <div class="box-body text-center">
              {{sheet.body|raw}}
            </div><!-- /.box-body -->
        </div><!-- /.box -->
    </div><!-- /.row -->
{% endblock %}
